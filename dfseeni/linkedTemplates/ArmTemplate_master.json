{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfseeni"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"CosmosDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDb'"
		},
		"seenistorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'seenistorage'"
		},
		"Trigger_properties_PL_SRC_TGT_copy1_parameters_FileName": {
			"type": "string",
			"defaultValue": "Customer.txt"
		},
		"Trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/934f567d-3ae2-4692-a817-07d7702480de/resourceGroups/dfstorage/providers/Microsoft.Storage/storageAccounts/seenistorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfseeni_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"CosmosDb_connectionString": {
						"value": "[parameters('CosmosDb_connectionString')]"
					},
					"seenistorage_connectionString": {
						"value": "[parameters('seenistorage_connectionString')]"
					},
					"Trigger_properties_PL_SRC_TGT_copy1_parameters_FileName": {
						"value": "[parameters('Trigger_properties_PL_SRC_TGT_copy1_parameters_FileName')]"
					},
					"Trigger_properties_typeProperties_scope": {
						"value": "[parameters('Trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}